<style>
  a li {
    width: 20px;
  }
</style>
<div>

  <div class="fixed-action-btn">
    <%= link_to new_memberships_path, class: 'btn-floating red' do %>
        <i class="large material-icons">add</i>
    <% end %>
  </div>
  <table class="highlight responsive-table" id="memberships">
    <thead>
    <th>MARK BOUNDARY</th>
    <th>LEVEl</th>
    <th>DISCOUNT RATE</th>
    <th></th>
    </thead>
    <tbody>
    <% @memberships.each do |membership| %>
        <tr>
          <td><%= membership.mark_boundary %></td>
          <td><%= membership.level %></td>
          <td><%= membership.discount_rate %></td>
          <td>
            <a class="" href="memberships/edit?id=<%= membership.id %>">
              <i class="material-icons">mode_edit</i>
            </a>
          </td>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>
<script>
    $( document ).on('turbolinks:load', function() {
        $('#memberships').dataTable()
        $('select').material_select();
    });
</script>